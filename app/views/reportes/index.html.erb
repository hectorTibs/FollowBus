<p id="notice"><%= notice %></p>

<h1>Reportes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Edad</th>
      <th>Sexo</th>
      <th>Hora</th>
      <th>Rutaid</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reportes.each do |reporte| %>
      <tr>
        <td><%= reporte.nombre %></td>
        <td><%= reporte.edad %></td>
        <td><%= reporte.sexo %></td>
        <td><%= reporte.hora %></td>
        <td><%= reporte.rutaid %></td>
        <td><%= reporte.descripcion %></td>
        <td><%= link_to 'Show', reporte %></td>
        <td><%= link_to 'Edit', edit_reporte_path(reporte) %></td>
        <td><%= link_to 'Destroy', reporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reporte', new_reporte_path %>
